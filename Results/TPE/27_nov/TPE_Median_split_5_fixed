    config = {
        "l1": trial.suggest_categorical("l1", [128, 256]),
        "l2": trial.suggest_categorical("l2", [64, 128]),
        "lr": trial.suggest_categorical("lr", [1e-3, 5e-4, 1e-4]),
        "batch_size": trial.suggest_categorical("batch_size", [32, 64])
    }
